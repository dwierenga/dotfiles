NO_COLOR='\[\e[0m\]'
#################################################################################################





#if [ -n "$SHORT_DIR" ]; then
  #DIR_FORMAT="${BOLD_CYAN_ON_BLACK}\W${NO_COLOR}"
#else
  #DIR_FORMAT="${BOLD_CYAN_ON_BLACK}\w${NO_COLOR}"
#fi


if [ -n "${STY}" ]; then
  SCREEN_NUM="(${BOLD_BLUE_ON_BLACK}$WINDOW${NO_COLOR})"
fi



## colorize the username
## Root user is always red, and always capitalized
## dan/dwierenga is always blue
## anyone else (value, perforce, etc.) is always red
IS_MYSELF=`echo $USER | grep -o 'dan|dwierenga'`
if [ "$USER" = "root" ]; then
  PROMPT_USER="${BOLD_RED_ON_BLACK}ROOT${NO_COLOR}"
elif [ $IS_MYSELF  ]; then
  SELF_NAME=`echo $USER| cut -f 1 -d "i"`
  PROMPT_USER="${BOLD_CYAN_ON_BLACK}$SELF_NAME${NO_COLOR}"         
else
  PROMPT_USER="${BOLD_RED_ON_BLACK}\u${NO_COLOR}"
fi

TITLEBAR='\[\e]2;$(hostname | cut -d . -f 1,2) \w\a\]'
PROMPT_HOST="${NORMAL_GREEN_ON_BLACK}\H${NO_COLOR}"
STOPPED_JOBS="${BOLD_GREEN_ON_BLACK}[\j]${NO_COLOR}"
AT_SIGN="${BOLD_WHITE_ON_BLACK}@${NO_COLOR}"
AMPM_TIME="${BOLD_RED_ON_BLACK}(\@)${NO_COLOR}"
HH24_MM_SS_TIME="${BOLD_RED_ON_BLACK}(\t)${NO_COLOR}"

PS1="${TITLEBAR}${NO_COLOR}${SCREEN_NUM} ${PROMPT_USER}${AT_SIGN}${PROMPT_HOST} ${HH24_MM_SS_TIME} \$(pwd) \n${STOPPED_JOBS}\$ ${NO_COLOR}"

if [ "$TERM" = "cons25" ]; then #cons25 is the default login console
  PS1='\u@\h \W \$'
fi 

#if [ "$TERM" = "screen" ]; then
#  if [ -n "$SCREEN_TITLE_CHANGE" ]; then
    #export PROMPT_COMMAND='screen -X title "$USER@$SHORT_HOST $PWD"'
#    export PROMPT_COMMAND='echo -n -e "\033k\033\134"'
#  fi
#fi
##### END PROMPT ##########################

PS1="${TITLEBAR}\n[\t] \w \$ "
